<Window x:Class="GB_ServerManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GB_ServerManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Grid Background="DarkOliveGreen">
            <StackPanel>
                <Button x:Name="btnHome" FontFamily="Segoe MDL2 Assets" FontSize="24" Background="Transparent" Padding="0,5,0,5">&#xEA8A;</Button>
                <Button x:Name="btnServer" FontFamily="Segoe MDL2 Assets" FontSize="24" Background="Transparent" Padding="0,5,0,5">&#xE12B;</Button>
            </StackPanel>
        </Grid>
        <Frame x:Name="frame" Grid.Column="1">
            
        </Frame>
    </Grid>
</Window>
